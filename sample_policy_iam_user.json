{
    "Version": "2012-10-17",
    "Statement": [
         {
            "Sid": "AllObjectActions",
            "Effect": "Allow",
            "Action": ["s3:GetObject"],
            "Resource": ["arn:aws:s3:::privatebucketname/*"]
        },
         {
            "Sid": "AllObjectActions",
            "Effect": "Allow",
            "Action": "s3:*Object",
            "Resource": ["arn:aws:s3:::publicbucketname/*"]
        }
    ]
}